<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1>Search Results</h1>
        </div>
    </div>
</div>          

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <% if(guest != "") {%>
                 <% guest.forEach(function(guest, index) { %>
                    <span><%- guest.name %></span>
                    <span><%- guest.phone_number %></span>
                    <span><%- guest.email %></span>
                    <span><%- guest.date %></span>
                    <span><%- guest.room_number %></span>
            <%}) %>    
            <% } else { %>
                <p>No people found</p>
            <% } %>
        </div>
    </div>
</div>  
